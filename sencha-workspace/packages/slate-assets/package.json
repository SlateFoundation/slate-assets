{
    "name": "slate-assets",
    "sencha": {
        "version": "1.0.0",
        "namespace": "Slate.assets",
        "type": "code",
        "framework": "ext",
        "toolkit": "classic",
        "theme": "theme-neptune",
        "requires": [
            "jarvus-lightbox"
        ],
        "classpath": [
            "${package.dir}/src"
        ],
        "overrides": [
            "${package.dir}/overrides"
        ],
        "sass": {
            "namespace": "Slate.assets",
            "etc": [
                "${package.dir}/sass/etc/all.scss"
            ]
        },
        "compressor": {
            "polyfills": "none"
        },
        "output": {
            "base": "${package.dir}/build",
            "js": {
                "version": "ES6"
            }
        }
    }
}